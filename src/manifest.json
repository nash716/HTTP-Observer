{
	"name": "HTTP Observer",
	"version": "0.33",
	"manifest_version": 2,
	"icons": {
		"64": "icon64.png",
		"48": "icon48.png",
		"128": "icon128.png",
		"32": "icon32.png",
		"16": "icon16.png"
	},
	"browser_action": {
		"default_icon": "icon128.png"
	},
	"description": "HTTPヘッダを確認することが出来る拡張機能です。",
	"background": {
		"scripts": [ "background.js" ]
	},
	"permissions": [
		"webRequest",
		"windows",
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}